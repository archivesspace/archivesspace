	<queryString>
		<![CDATA[SELECT
     T2.`id` AS subject_id,
     T2.`title` AS subjectTerm,
     GetTermType(T2.`id`) AS subjectTermType,
     GetEnumValue(T2.`source_id`) AS subjectSource
FROM
     `subject_rlshp` T1
INNER JOIN
     `subject` T2 ON T1.`subject_id` = T2.`id`
WHERE
     GetResourceId(T1.`resource_id`, T1.`archival_object_id`) = $P{resourceId}
GROUP BY
    T1.`subject_id`]]>
	</queryString>
