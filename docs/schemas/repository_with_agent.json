{
  "$schema": "http://www.archivesspace.org/archivesspace.json",
  "version": 1,
  "type": "object",
  "uri": "/repositories/with_agent",
  "properties": {
    "uri": {
      "type": "string",
      "required": false
    },
    "repository": {
      "type": "JSONModel(:repository) object",
      "ifmissing": "error"
    },
    "agent_representation": {
      "type": "JSONModel(:agent_corporate_entity) object"
    },
    "lock_version": {
      "type": [
        "integer",
        "string"
      ],
      "required": false
    },
    "jsonmodel_type": {
      "type": "string",
      "ifmissing": "error"
    },
    "created_by": {
      "type": "string",
      "readonly": true
    },
    "last_modified_by": {
      "type": "string",
      "readonly": true
    },
    "user_mtime": {
      "type": "date-time",
      "readonly": true
    },
    "system_mtime": {
      "type": "date-time",
      "readonly": true
    },
    "create_time": {
      "type": "date-time",
      "readonly": true
    }
  }
}