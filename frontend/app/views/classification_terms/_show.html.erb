<section id="<%= section_id %>">
  <h3><%= I18n.t("classification_term._plural") %></h3>
  <div class="details">
    <div id="<%= section_id %>_classification_terms">
      <div class="subrecord-form-dummy">
        <div class="subrecord-form-container">
          <div class="subrecord-form-fields">
            <div class="row">
              <div class="col-md-4 token-list">
              <% classification_terms.each do | classification_term | %>
                <%= render_token :object => classification_term,
                :label => classification_term["_resolved"]["title"],
                :type => classification_term["_resolved"]["jsonmodel_type"],
                :uri => classification_term["_resolved"]["uri"] %>
              <% end %>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
