<section id="<%= section_id %>">
  <h3><%= t("classification._singular") %></h3>
  <div class="details">
    <div id="<%= section_id %>_classification">
      <div class="subrecord-form-dummy">
        <div class="subrecord-form-container">
          <div class="subrecord-form-fields">
            <div class="row">
              <div class="col-md-4 token-list">
                <%= render_token :object => classification, 
                :label => classification["_resolved"]["title"],
                :type => classification["_resolved"]["jsonmodel_type"],
                :uri => classification["_resolved"]["uri"] %>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
