<%= render_aspace_partial :partial => "related_agents/template" %>

<section id="related_agents" class="subrecord-form">
  <h3 class="subrecord-form-heading"><%= I18n.t("related_agent._plural") %></h3>
  <div class="subrecord-form-container">
    <%= context.list_for(context["related_agents"], "related_agents[]") do |related_agent| %>
      <% context.emit_template(related_agent["jsonmodel_type"], :readonly => true) %>
    <% end %>
  </div>
</section>

