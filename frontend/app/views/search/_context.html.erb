<% ancestors = context_ancestor(result) %>
<% if ancestors != [''] %>
  <% ancestors.reverse.each_with_index do |ancestor, i| %>
    <%= resolve_readonly_link_to((get_ancestor_title(ancestor).html_safe), ancestor) %>
    <% if i < ancestors.length - 1 %>
      <%= context_separator(result) %>
    <% end %>
  <% end %>
<% end %>
