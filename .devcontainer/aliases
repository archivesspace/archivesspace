## BUILD
alias api="supervisord -c supervisord/api.conf"
alias brb="build/run bootstrap"
alias brm="build/run db:migrate"
alias brr="build/run solr:reset"
alias run="supervisord -c supervisord/archivesspace.conf"
## GIT
alias gdc="git diff --cached"
alias gdf="git diff"
alias gfa="git fetch --all"
alias gst="git status"
## MYSQL
alias myd="mycli --user=as --password=as123 --database=asdev --auto-vertical-output"
alias myt="mycli --user=as --password=as123 --database=astest --auto-vertical-output"
## SERVICES
alias srv="sudo service mariadb start && sudo service solr start"
