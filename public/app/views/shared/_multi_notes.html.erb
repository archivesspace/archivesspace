<% types.each do |type|
	(@result.notes[type] || []).each do |note_struct|
	   if !note_struct['note_text'].blank? %>
	    <div class="note">
	      <%= render  partial: 'shared/single_note',
	          locals: {:type => type, :note_struct => note_struct, :heading_size => 'h3', :sub_heading_size => 'h4'}  %>
	    </div>
	   <% end %>
	<% end %>
<% end %>
