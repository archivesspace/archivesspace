<figure data-rep-file-version-wrapper>
  <a href="<%= uri %>" target="new" title="<%= t('digital_object._public.link')%>">
    <img src="<%= uri %>" alt="<%= caption.blank? ? '' : caption %>">
  </a>
  <% unless caption.blank? %>
    <figcaption class="bg-lightgray"><%= caption %></figcaption>
  <% end %>
</figure>
