<% if @result && @result.respond_to?(:metadata) %>
  <script type="application/ld+json">
    <%= JSON.pretty_generate(@result.metadata).html_safe %>
  </script>
<% elsif @result && @result['jsonmodel_type'] == 'repository' %>
  <script type="application/ld+json">
    <%= JSON.pretty_generate(@metadata).html_safe %>
  </script>
<% end %>
