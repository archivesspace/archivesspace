{
  "name": "archivesspace-frontends",
  "version": "0.1.0",
  "description": "Lint and format js, css, and html in frontend/ and public/",
  "scripts": {
    "pre-commit-lint": "eslint",
    "pre-commit-format": "pretty-quick --staged",
    "ci-lint": "eslint frontend/ public/ -f compact",
    "ci-format": "prettier --check frontend/ public/",
    "lint-output": "node temp-lint-output.js",
    "lint-output-analysis": "node temp-lint-analysis.js"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.{js,html,less,css,scss}": "pretty-quick --staged"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1"
  },
  "license": "ECL-2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:archivesspace/archivesspace.git"
  }
}
